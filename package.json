    "rollup": "^2.17.1",
    "standard": "^14.3.4"
  },
  "dependencies": {
    "@ipld/dag-cbor": "1.0.6",
    "class-is": "^1.1.0",
    "lodash.transform": "^4.6.0",
    "multiformats": "^1.0.7"
  },
  "repository": {
    "type": "git",
